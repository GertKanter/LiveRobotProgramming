(machine greeting	(state say 		(onentry 			[robot serviceALTextToSpeech async say: 'Hello! I will move thanks to L R P']))	(state prepareToWalk		(onentry			[robot serviceALMotion wakeUp.			robot serviceALRobotPosture async				goToPosture: 'Stand' speed: 0.5]))	(state walk		(onentry			[robot serviceALMotion async moveToX: 0.5 y:0 theta: 0]))	(state goodbye		(onentry			[robot serviceALTextToSpeech async say: 'Thank you for watching this demo!'.			robot serviceMyAnimation async waveHand			]))	(state sit		(onentry			[robot serviceALRobotPosture async				goToPosture: 'Sit' speed: 0.5]))	(state rest (onentry [robot serviceALMotion rest]))		(ontime 4000 say -> prepareToWalk)	(on [robot serviceALRobotPosture lastProcess isFinished] prepareToWalk -> walk)	(on [robot serviceALMotion lastProcess isFinished] walk -> goodbye)	(on [robot serviceMyAnimation lastProcess isFinished] goodbye -> sit)	(on [robot serviceALRobotPosture lastProcess isFinished] sit -> rest))(spawn greeting say)