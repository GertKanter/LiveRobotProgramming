;; A contrived example to show nested machines;; and the UI's ability to follow state stanges(machine root	(state r1		(machine nest1 			(state n1				(machine nest2 (state n1n2))				(onentry (spawn nest2 n1n2))				)			(ontime 500 n1->n2)			(state n2				(machine nest3 (state n2n3))				(onentry (spawn nest3 n2n3))				)			(state n3)			(ontime 500 n2->n3))		(onentry (spawn nest1 n1))		)	(state r2)	(ontime 2000 r1->r2)	(ontime 500 r2->r1))(spawn root r1)