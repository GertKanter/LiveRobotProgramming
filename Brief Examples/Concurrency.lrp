;; Concurrency using top-level machines(machine m	(state s)	(state t)	(ontime 1500 s->t)	(ontime 1500 t->s))(machine n	(state a)	(state b)	(state c)	(ontime 2000 a->b)	(ontime 2000 b->c)	(ontime 2000 c->a))(machine o	(state p		(onentry (spawn m s))		(onentry (spawn n a))))(spawn o p)